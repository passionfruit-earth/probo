    "@tailwindcss/vite": "^4.1.7",
    "@types/node": "^25.2.3",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/react-relay": "^18.2.1",
    "@types/relay-runtime": "^20.1.1",
    "@vitejs/plugin-react": "^5.1.4",
    "babel-plugin-relay": "^20.1.1",
    "eslint": "^10.0.0",
    "graphql": "^16.11.0",
    "relay-compiler": "^20.1.1",
