{
  "name": "@probo/n8n-nodes-probo",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "build": "n8n-node build",
    "build:watch": "tsc --watch",
    "dev": "n8n-node dev",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
    "release": "npm run build && npm run lint && npm publish --access public",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "author": {
    "name": "Probo Inc",
    "email": "hello@getprobo.com"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "strict": true,
    "credentials": [
      "dist/credentials/ProboApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Probo/Probo.node.js"
    ]
  },
  "license": "MIT",
  "description": "n8n nodes for integrating with the Probo compliance platform API",
  "keywords": [
    "n8n",
    "n8n-nodes",
    "probo",
    "compliance",
    "soc2",
    "automation",
    "n8n-community-node-package"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getprobo/probo.git",
    "directory": "packages/n8n-node"
  },
  "bugs": {
    "url": "https://github.com/getprobo/probo/issues"
  },
  "homepage": "https://github.com/getprobo/probo#README",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "devDependencies": {
    "@n8n/node-cli": "^0.17.0",
    "eslint": "^9.39.2"
  }
}
